##脚本：3 ≤HA≤ 100
let errmsg = nil;
if(HA!= nil){
   HA=decimal(HA);
   if(3<=HA && HA<=100){
       return true;
   }else{
       errmsg = "需满足公式 3≤HA≤100";
       return errmsg;
   }
}
return true;

##脚本：3 ≤HC≤ HA+HB_1
let errmsg = nil;
if(HC!= nil && HA!= nil && HB_1!= nil){
   HC=decimal(HC);
   HA=decimal(HA);
   HB_1=decimal(HB_1);
   let HA=HA+HB_1;
   if(3<=HC && HC<=HA){
       return true;
   }else{
       errmsg = "需满足公式 3≤HC≤HA+HB_1";
       return errmsg;
   }
}
return true;

