let p = productSearchSkuInfo;scMax = nil;lMin = nil;for i in p.specList {   L = nil;   SC = nil;   let lValue = nil;   let scValue = nil;   if(attrSpec != nil){      for j in attrSpec {          if(j.enSpecName == "L"){             L = j.enSpecName;             lValue = j.attrSpecValue;          }          if(j.enSpecName == "SC"){             SC = j.enSpecName;             scValue = j.attrSpecValue;             ##scValueId = j.attrSpecValueId;          }      }   }if(specType >= 6 && specType <= 10 ){        if(L != nil && SC == nil){           scMax = lValue - 8;        }elsif (L == nil && SC != nil){           lMin = scValue + 8;        }elsif(L != nil && SC != nil){           scMax = lValue - 8;           lMin = scValue + 8;        }   }   elsif(specType >= 12 && specType <= 25){        if(L != nil && SC == nil){            scMax = lValue - 10;        }elsif(L == nil && SC != nil){            lMin = scValue + 10;        }elsif(L != nil && SC != nil){            scMax = lValue - 10;            lMin = scValue + 10;        }   }  elsif(specType > 25){       if(L != nil && SC == nil){           scMax = lValue - 15;       }elsif(L == nil && SC != nil){           lMin = scValue + 15;       }elsif(L != nil && SC != nil){           scMax = lValue - 15;           lMin = scValue + 15;       }   }}for m in p.specList{  for n in m.specRangeList {        if(m.enSpecName == "L"){          if(lMin != nil){            n.minValue = lMin;          }          n.maxValue = n.maxValue;        }        if(m.enSpecName == "SC"){          if(scMax != nil){            n.maxValue = scMax;          }          n.minValue = n.minValue;        }    }}return p;